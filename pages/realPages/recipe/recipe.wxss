/* pages/realPages/mainUi/mainUi.wxss */
page{
  display: flex;
  flex-direction: column;
  height: 100%;
}.outest-search{
  position: fixed;
  overflow-x: hidden;
  width: 100%;
  height: 100%;
  z-index: 10;
}.outest_navbar{
  position: fixed;
  overflow-x: hidden;
  width: 100%;
  height: 100%;
  top:93rpx;
  z-index: 10;
}
.weui-search-bar {
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #EFEFF4;
  border-top: 1rpx solid #D7D6DC;
  border-bottom: 1rpx solid #D7D6DC;
}
.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
  border-radius: 5px;
  background: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}
.weui-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}
.weui-search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}
.weui-icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}.weui-search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #09BB07;
  white-space: nowrap;
}.detail{
  width: 750rpx;
  display: flex;
  flex-direction: row;
  margin-top: 30rpx;
}.heat_style{
  color:rgb(47, 255, 54);
  width: 30%;
  text-align: right;
  margin-right: 5rpx;
}.name_style{
  width: 60%;
  margin-left: 50rpx;
  font-weight: bold;
}.div_line{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
  margin-top: 30rpx;
}.qianka{
  width: 10%;
  font-size: 75%;
  color:rgb(47, 255, 54);
  margin-top: 8rpx;
  margin-right: 50rpx;
}.navbar{
  flex: none;
  display: flex;
  background: #fff;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}
.navbar .item.active{
  color: #FFCC00;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #FFCC00;
}.fir_page{
  margin-top: 160rpx;
}